{
  "name": "kamelher/laravel-settings",
  "description": "Simple Settings package for laravel application",
  "keywords": [
    "Laravel",
    "Settings"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "KamelEddine Heraguemi",
      "email": "kamel.heragmi@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "KAMEL\\Settings\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "KAMEL\\Settings\\App\\Providers\\SettingServiceProvider"
      ],
      "aliases": {
        "Settings": "KAMEL\\Settings\\App\\Facades\\Setting"
      }
    }
  }
}
